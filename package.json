{
  "name": "learn-turbo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build:elm": "mkdir -p dist && elm-watch make",
    "build:html": "mkdir -p dist && cp src/index.html dist/index.html",
    "build:js": "mkdir -p dist && tsc --outDir dist",
    "test:elm": "elm-test --seed ${ELM_TEST_SEED:-1234567890}",
    "dev": "hivemind",
    "dev:elm": "elm-watch hot",
    "dev:html": "python3 -m http.server -d dist",
    "format:elm": "elm-format --yes $(find src -type f -name '*.elm')",
    "format:ts": "prettier -w $(find src -type f -name '*.ts')",
    "format:html": "prettier -w $(find src -type f -name '*.html')"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "elm-watch": "^1.1.2",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  }
}
